{
   "private": true,
   "name": "typescript-starter",
   "version": "1.0.0",
   "author": "ProxityStudios",
   "license": "MIT",
   "homepage": "https://github.com/ProxityStudios/typescript-starter/blob/main/README.md",
   "readme": "./README.md",
   "bugs": {
      "url": "https://github.com/ProxityStudios/typescript-starter/issues"
   },
   "scripts": {
      "start": "node dist/main.js --experimental-specifier-resolution=node --env production",
      "dev": "ts-node-dev --respawn src/main.ts --rs --debounce 2000 --experimental-specifier-resolution=node",
      "build": "concurrently -n fix \"tsc --project tsconfig.json\"",
      "fix": "concurrently \"npm run fix:prettier\"  \"npm run fix:lint\"",
      "fix:prettier": "prettier \"src/**/*.ts\" --write",
      "fix:eslint": "eslint \"src/**/*.ts\" --fix"
   },
   "devDependencies": {
      "@types/node": "^20.10.5",
      "@types/yargs": "^17.0.32",
      "@typescript-eslint/eslint-plugin": "^6.15.0",
      "@typescript-eslint/parser": "^6.15.0",
      "eslint": "^8.56.0",
      "prettier": "^3.1.1",
      "ts-node-dev": "^2.0.0",
      "typescript": "^5.3.3"
   },
   "dependencies": {
      "concurrently": "^8.2.2",
      "dotenv": "^16.3.1",
      "tslog": "^4.9.2",
      "yargs": "^17.7.2"
   }
}
